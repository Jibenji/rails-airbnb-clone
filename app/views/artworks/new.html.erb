<%= simple_form_for(@artwork) do |f| %>
  <%= f.input :art_category %>
  <%= f.input :description %>
  <%= f.input :size %>
  <%= f.input :material_description %>
  <%= f.input :photo_artwork %>
  <%= f.input :photo_artwork_cache, as: :hidden %>
  <%= f.input :framed %>
  <%= f.input :price %>
  <%= f.submit %>
<% end %>


