<%= simple_form_for [current_account, @user] do |f| %>
<%= f.error_notification %>

<div class="form-inputs">
  <%= f.input :phone %>
  <%= f.input :address %>
  <%= f.input :city %>
  <%= f.input :bio %>
  <%= f.input :description %>
  <%= f.input :alias %>
  <%= f.input :picture_avatar %>
  <%= f.input :bank_account %>
  <%= f.input :picture_artist_banner %>
</div>

<div class="form-actions">
  <%= f.button :submit, "Update your profile" %>
</div>

<% end %>
